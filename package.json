{
	"name": "fury",
	"displayName": "Fury",
	"description": "Experimental dependency manager and build tool for Scala.",
	"version": "0.0.1",
	"publisher": "Propensive",
	"icon": "logo.png",
	"engines": {
		"vscode": "^1.43.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/propensive/fury-vscode"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:fury.helloWorld",
		"onView:furyCommands"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "fury-explorer",
					"title": "Fury",
					"icon": "logo.svg"
				}
			]
		},
		"views": {
			"fury-explorer": [
				{
					"id": "furyCommands",
					"name": "Commands"
				}
			]
		},
		"commands": [
			{
				"command": "fury.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.43.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}